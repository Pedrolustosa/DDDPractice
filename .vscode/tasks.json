            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/DDDPractice/DDDPractice.Application/DDDPractice.Application.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
